<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.textfield.TextInputLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/andromedaTIL"
        app:hintTextAppearance="@style/TitleSmallBoldDefault"
        app:errorTextAppearance="@style/TitleSmallBoldError"
        app:hintTextColor="?attr/fill_active_primary"
        app:errorTextColor="?attr/fill_active_secondary"
        app:boxStrokeColor="?attr/fill_active_secondary"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <EditText
            android:id="@+id/andromedaET"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:text="@tools:sample/first_names" />
</com.google.android.material.textfield.TextInputLayout>
